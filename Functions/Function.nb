    "\[IndentingNewLine]", 
    RowBox[{
     RowBox[{"S", "[", 
      RowBox[{"[", 
       RowBox[{"All", ",", "1"}], "]"}], "]"}], "=", "S0"}], ";", 
    "\[IndentingNewLine]", 
    RowBox[{"dW", "=", 
     RowBox[{
      RowBox[{"Sqrt", "[", "dT", "]"}], "*", 
