Cell[BoxData[
 RowBox[{
  RowBox[{"Simul", "[", 
   RowBox[{
   "M_", ",", "callput_", ",", "n_", ",", "T_", ",", "K_", ",", "R_", ",", 
    "sigma_", ",", "S0_"}], "]"}], ":=", "\[IndentingNewLine]", 
  RowBox[{"(", 
   RowBox[{
    RowBox[{"dT", "=", 
     RowBox[{"1", "/", "n"}]}], ";", " ", "\[IndentingNewLine]", 
