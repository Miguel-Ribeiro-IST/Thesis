         RowBox[{"dW", "[", 
          RowBox[{"[", 
           RowBox[{"All", ",", "i"}], "]"}], "]"}]}]}]}]}], "]"}], " ", ";", 
    "\[IndentingNewLine]", 
    RowBox[{"Clear", "[", "dW", "]"}], ";", "\[IndentingNewLine]", 
    RowBox[{
     RowBox[{"Payoff", "[", "stock_", "]"}], ":=", 
     RowBox[{"If", "[", 
      RowBox[{
       RowBox[{"callput", "==", "\"\<Put\>\""}], ",", 
       RowBox[{"Max", "[", 
        RowBox[{
         RowBox[{"K", "-", "stock"}], ",", "0"}], "]"}], ",", 
       RowBox[{"Max", "[", 
        RowBox[{
         RowBox[{"stock", "-", "K"}], ",", "0"}], "]"}]}], "]"}]}], ";", " ", 
    "\[IndentingNewLine]", 
    RowBox[{"cfreal", "=", 
     RowBox[{"Table", "[", 
      RowBox[{"0", ",", "M", ",", 
       RowBox[{"n", "*", "T"}]}], "]"}]}], ";", " ", "\[IndentingNewLine]", 
